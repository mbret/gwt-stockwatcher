<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <servlet>
        <servlet-name>com.gettingstarted.gettingstarted gettingstartedService</servlet-name>
        <servlet-class>com.gwt.server.gettingstartedServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stockPriceServiceImpl</servlet-name>
        <servlet-class>com.gwt.server.gettingstartedServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>com.gettingstarted.gettingstarted gettingstartedService</servlet-name>
        <url-pattern>/gettingstarted/gettingstartedService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stockPriceServiceImpl</servlet-name>
        <url-pattern>/stockwatcher/stockPrices</url-pattern>
    </servlet-mapping>
</web-app>
